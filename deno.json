{
  "tasks": {
    "dev": "NODE_ENV=dev deno -A --env --node-modules-dir server.ts",
    "build": "deno -A --env --node-modules-dir npm:astro build",
    "serve": "deno -A --env --node-modules-dir server.ts",
    "reset": "rm -rf deno.lock node_modules drizzle dist .astro",
    "astro:add": "deno -A --env --node-modules-dir astro add",
    "drizzle:generate": "deno -A --env --node-modules-dir npm:drizzle-kit generate",
    "drizzle:migrate": "deno -A --env --node-modules-dir npm:drizzle-kit migrate",
    "drizzle:pull": "deno -A --env --node-modules-dir npm:drizzle-kit pull",
    "drizzle:push": "deno task drizzle:pull && deno task drizzle:generate && deno task drizzle:migrate",
    "drizzle:check": "deno -A --env --node-modules-dir npm:drizzle-kit check",
    "drizzle:up": "deno -A --env --node-modules-dir npm:drizzle-kit up",
    "drizzle:drop": "deno -A --env --node-modules-dir npm:drizzle-kit drop",
    "drizzle:studio": "deno -A --env --node-modules-dir npm:drizzle-kit studio",
    "drizzle:export": "deno -A --env --node-modules-dir npm:drizzle-kit export"
  },
  "imports": {
    "@/": "./src/",
    "@astrojs/partytown": "npm:@astrojs/partytown@^2.1.4",
    "@astrojs/solid-js": "npm:@astrojs/solid-js@^5.1.3",
    "@deno/astro-adapter": "npm:@deno/astro-adapter@^0.3.2",
    "@std/bytes": "jsr:@std/bytes@^1.0.6",
    "@std/http": "jsr:@std/http@^1.0.21",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.17",
    "@tiptap/core": "npm:@tiptap/core@^3.10.7",
    "@tiptap/extension-blockquote": "npm:@tiptap/extension-blockquote@^3.10.4",
    "@tiptap/extension-bold": "npm:@tiptap/extension-bold@^3.10.4",
    "@tiptap/extension-bubble-menu": "npm:@tiptap/extension-bubble-menu@^3.10.4",
    "@tiptap/extension-code": "npm:@tiptap/extension-code@^3.10.4",
    "@tiptap/extension-document": "npm:@tiptap/extension-document@^3.10.4",
    "@tiptap/extension-drag-handle": "npm:@tiptap/extension-drag-handle@^3.10.4",
    "@tiptap/extension-file-handler": "npm:@tiptap/extension-file-handler@^3.10.4",
    "@tiptap/extension-hard-break": "npm:@tiptap/extension-hard-break@^3.10.4",
    "@tiptap/extension-heading": "npm:@tiptap/extension-heading@^3.10.4",
    "@tiptap/extension-horizontal-rule": "npm:@tiptap/extension-horizontal-rule@^3.10.4",
    "@tiptap/extension-image": "npm:@tiptap/extension-image@^3.10.4",
    "@tiptap/extension-italic": "npm:@tiptap/extension-italic@^3.10.4",
    "@tiptap/extension-link": "npm:@tiptap/extension-link@^3.10.4",
    "@tiptap/extension-list": "npm:@tiptap/extension-list@^3.10.4",
    "@tiptap/extension-mathematics": "npm:@tiptap/extension-mathematics@^3.10.4",
    "@tiptap/extension-paragraph": "npm:@tiptap/extension-paragraph@^3.10.4",
    "@tiptap/extension-strike": "npm:@tiptap/extension-strike@^3.10.4",
    "@tiptap/extension-subscript": "npm:@tiptap/extension-subscript@^3.10.4",
    "@tiptap/extension-superscript": "npm:@tiptap/extension-superscript@^3.10.4",
    "@tiptap/extension-table": "npm:@tiptap/extension-table@^3.10.4",
    "@tiptap/extension-text": "npm:@tiptap/extension-text@^3.10.4",
    "@tiptap/extension-text-style": "npm:@tiptap/extension-text-style@^3.10.4",
    "@tiptap/extension-typography": "npm:@tiptap/extension-typography@^3.10.4",
    "@tiptap/extension-underline": "npm:@tiptap/extension-underline@^3.10.4",
    "@tiptap/extension-youtube": "npm:@tiptap/extension-youtube@^3.10.4",
    "@tiptap/extensions": "npm:@tiptap/extensions@^3.10.4",
    "@unocss/preset-wind4": "npm:@unocss/preset-wind4@^66.5.10",
    "astro": "npm:astro@^5.16.3",
    "drizzle-graphql": "npm:drizzle-graphql@^0.8.5",
    "drizzle-kit": "npm:drizzle-kit@^0.31.6",
    "drizzle-orm": "npm:drizzle-orm@^0.44.7",
    "graphql-yoga": "npm:graphql-yoga@^5.16.2",
    "postgres": "npm:postgres@^3.4.7",
    "solid-js": "npm:solid-js@^1.9.10",
    "unocss": "npm:unocss@^66.5.10",
    "vite": "npm:vite@^7.2.2"
  },
  "nodeModulesDir": "auto"
}
